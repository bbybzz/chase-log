{
    "version": 2,
    "builds": [
        {
            "src": "api/creds.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/creds",
            "dest": "api/creds.js"
        },
        {
            "src": "/(.*)",
            "dest": "index.html"
        }
    ]
}
